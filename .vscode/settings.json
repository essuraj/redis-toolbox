// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "node_modules": true,
        "typings": true,
        "**/*.map": true,
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "editor.glyphMargin": true,
    "sass.validate": false,
    "less.validate": false
}